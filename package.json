{
  "name": "tbar",
  "version": "0.1.0",
  "description": "An i3status replacement written in TypeScript.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "tbar": "lib/cli.js"
  },
  "scripts": {
    "compile": "tsc -p .",
    "lint": "tslint -p .",
    "test": "ava"
  },
  "ava": {
    "files": [
      "src/**/*.spec.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomas-crane/tbar.git"
  },
  "keywords": [
    "i3",
    "i3wm",
    "status-bar"
  ],
  "author": "Thomas Crane <thomasgcrane@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thomas-crane/tbar/issues"
  },
  "homepage": "https://github.com/thomas-crane/tbar#readme",
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.6.0",
    "ava": "^3.12.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "minimist": "^1.2.6",
    "tiny-typed-emitter": "^2.0.2"
  }
}
